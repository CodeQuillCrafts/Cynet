generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model participants {
  id                  String   @id @default(auto()) @map("_id") @db.ObjectId
  course              String
  name                String
  transactionId       String
  isTransactionVerify Boolean  @default(false)
  createdAt           DateTime @default(now())
}
